<robot name="LANCE">
    <link name="link1" />
    <link name="link2" />
    <link name="link3" />
    <link name="link4" />
    <link name="link5" />

    <joint name="base" type="continuous">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0 -35 64.4" rpy="0 0 0.78539816339"/>
        <axis xyz="0 0 1"/>
    </joint>

    <joint name="shoulder" type="continuous">
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="25 0 28.1" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>

    <joint name="elbow" type="continuous">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0 322 0" rpy="0 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>

    <joint name="wrist" type="continuous">
        <parent link="link4"/>
        <child link="link5"/>
        <origin xyz="-9.5 308.5 -46.5" rpy="-1.570796 0 0"/>
        <axis xyz="1 0 0"/>
    </joint>

</robot>    